{% unless product == empty %}
                <script type='application/json' data-ptmproduct-json>
                    {{product | json}}
                </script>
            {% endunless %}
            {% if template contains 'page' %}
            <script type='application/json' data-ptmpage-json>
                {{page.id}}
            </script>
            {% endif %}
            <div id='pt-music' ptm-page='{{ template }}'></div>